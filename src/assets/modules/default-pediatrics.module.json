        },
        {
            "code": "RESEARCH",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Pediatric Research",
            "description": "Pediatric research participation, requires parental consent and minor assent per PREA/BPCA"
        },
        {
            "code": "EMERGENCY",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Emergency Care",
            "description": "Emergency care without consent when necessary to prevent serious harm"
        },
        {
            "code": "PUBLIC-HEALTH",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
