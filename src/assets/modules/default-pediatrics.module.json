    "$schema": "../schemas/data-segmentation-module.schema.json",
    "id": "default-pediatrics",
    "name": "Pediatric Healthcare Data Segmentation Module",
    "version": "1.0.0",
    "description": "Comprehensive data segmentation module for compliance with regulations governing pediatric healthcare data exchange, including HIPAA minor privacy provisions, state minor consent laws, mature minor doctrine, child abuse reporting requirements, and age-specific care considerations. This module provides hierarchical categories, policies mapped to specific regulatory sections, and extensive code set bindings using SNOMED CT (US Edition), CPT, ICD-10-CM, RxNorm, LOINC, and HL7 Medicinal Product Pediatric Use terminologies.",
    "enabled": true,
    "categories": [
        {
            "code": "PED-AGE",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Age-Based Pediatric Care",
            "description": "Pediatric healthcare information organized by age groups"
        },
        {
            "code": "PED-AGE-NEWBORN",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Newborn Care",
            "description": "Healthcare information for newborns (0-27 days), including preterm and term newborns",
            "parent": "PED-AGE"
        },
        {
            "code": "PED-AGE-INFANT",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Infant Care",
            "description": "Healthcare information for infants and toddlers (28 days - 23 months)",
            "parent": "PED-AGE"
        },
        {
            "code": "PED-AGE-CHILD",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Child Care",
            "description": "Healthcare information for children (2-11 years)",
            "parent": "PED-AGE"
        },
        {
            "code": "PED-AGE-ADOLESCENT",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Adolescent Care",
            "description": "Healthcare information for adolescents (12-17 years)",
            "parent": "PED-AGE"
        },
        {
            "code": "PED-SENSITIVE",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Sensitive Services",
            "description": "Sensitive healthcare services where minors may consent without parental involvement per state law"
        },
        {
            "code": "PED-SENSITIVE-REPRODUCTIVE",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Reproductive Health Services",
            "description": "Reproductive health services where minors may consent without parental involvement per state law",
            "parent": "PED-SENSITIVE"
        },
        {
            "code": "PED-SENSITIVE-MENTAL",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Mental Health Services",
            "description": "Mental health services where minors may consent without parental involvement per state law",
            "parent": "PED-SENSITIVE"
        },
        {
            "code": "PED-SENSITIVE-SUBSTANCE",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Substance Abuse Services",
            "description": "Substance abuse treatment services where minors may consent without parental involvement per state law",
            "parent": "PED-SENSITIVE"
        },
        {
            "code": "PED-ABUSE-REPORTING",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Child Abuse and Neglect Reporting",
            "description": "Records related to mandatory child abuse and neglect reporting under CAPTA"
        },
        {
            "code": "PED-RESEARCH",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Pediatric Research",
            "description": "Pediatric research participation records requiring parental consent and minor assent per PREA/BPCA"
        },
        {
            "code": "PED-MEDICATION-SAFETY",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Medication Safety",
            "description": "Medication safety records including child-resistant packaging requirements per PPPA"
        },
        {
            "code": "PED-FERPA-STUDENT",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
