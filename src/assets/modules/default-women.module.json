{
    "$schema": "../schemas/data-segmentation-module.schema.json",
    "id": "default-women",
    "name": "Women's Health Data Segmentation Module",
    "version": "1.0.0",
    "description": "Comprehensive data segmentation module for compliance with regulations governing women's health data exchange, including HIPAA reproductive health privacy provisions (2024), state reproductive privacy laws (California, Washington, Virginia, Illinois), Title X confidentiality requirements, and protections for sensitive women's health services. This module provides hierarchical categories, policies mapped to specific regulatory sections, and extensive code set bindings using SNOMED CT (US Edition), CPT, ICD-10-CM, RxNorm, and LOINC terminologies.",
    "enabled": true,
    "categories": [
        {
            "code": "WOMEN-REPRODUCTIVE",
            "system": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
            "name": "Reproductive Health Services",
            "description": "Reproductive health services including abortion, contraception, fertility treatment, and pregnancy loss care, protected under HIPAA reproductive health privacy rule and state laws"
        },
        {
